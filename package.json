{
    "name": "gethelp",
    "displayName": "GetHelp",
    "description": "A extension to do Google/StackOverflow search quickly",
    "version": "0.0.1",
    "publisher": "none",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Google", 
        "Search", 
        "GetHelp", 
        "Help",
        "StackOverflow"
    ],
    "license": "SEE LICENSE IN LICENSE.txt",
    "homepage": "https://github.com/krishnaanaril/GetHelp/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/krishnaanaril/GetHelp/issues",
        "email": "krishnamohan.a.m@gmail.com"
    },
    "icon": "assets/images/icon.png",
    "galleryBanner": {
        "color": "#ffffff",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/krishnaanaril/GetHelp.git"
    },
    "activationEvents": [
        "onCommand:extension.getHelpGoogle"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getHelpGoogle",
                "title": "GetHelp - Google"
            },
            {
                "command": "extension.getHelpSO",
                "title": "GetHelp - StackOverflow"
            }
        ],
        "menus": {
          "editor/context": [{
              "when": "editorHasSelection",
              "command": "extension.getHelpGoogle",
              "group": "navigation"
        },
        {
            "when": "editorHasSelection",
            "command": "extension.getHelpSO",
            "group": "navigation"
      }]
      }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
